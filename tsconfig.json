{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": ["cypress"],
    "rootDir": "./",
    "baseUrl": "./",
    "paths": {
      "@trzCy/*": ["./cypress/*"],
      "@trzData/*": ["./cypress/data/*"],
      "@trzFixture/*": ["./cypress/fixtures/*"],
      "@trzUtils/*": ["./cypress/utils/*"],
      "@trzSelectors/*": ["./cypress/utils/selectors/*"]
    }
  },
  "include": ["src", "cypress/**/*.ts", "cypress/**/*.cy.ts"],
  "references": []
}
